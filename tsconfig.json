{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/",
    "module": "esnext",
    "allowSyntheticDefaultImports": true,
    "preserveSymlinks": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "jsx": "react",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "removeComments": true,
    "sourceMap": true,
    "pretty": true
  }
}
